{
  "name": "@poker-god/mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "build": "eas build --platform all",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:preview": "eas build --profile preview --platform all",
    "build:production": "eas build --profile production --platform all",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:ios:wait": "eas submit --platform ios --latest --wait",
    "android": "expo start --android",
    "android:local": "powershell -ExecutionPolicy Bypass -File .\\start-android-local.ps1",
    "android:local:clear": "powershell -ExecutionPolicy Bypass -File .\\start-android-local.ps1 -Clear",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "solver:build": "npm run solver:export && npm run solver:postflop:build",
    "solver:postflop:build": "node tools/postflop-cfr-generator.mjs",
    "solver:export": "cargo run --release --manifest-path tools/cfr-export/Cargo.toml -- tools/poker-cfr/output/preflop-20-75000.bin src/solver/data/preflop-20bb.json && cargo run --release --manifest-path tools/cfr-export/Cargo.toml -- tools/poker-cfr/output/preflop-40-210000.bin src/solver/data/preflop-40bb.json && cargo run --release --manifest-path tools/cfr-export/Cargo.toml -- tools/poker-cfr/output/preflop-100-670000.bin src/solver/data/preflop-100bb.json",
    "solver:river:convert": "node tools/third_party/bridge/convert-river-strategy.mjs",
    "solver:river:build": "node tools/third_party/bridge/build-river-overrides.mjs",
    "solver:multiway:convert": "node tools/third_party/bridge/convert-slumbot-multiway.mjs",
    "solver:abtest:multiway": "node tools/eval/multiway-ab-test.mjs"
  },
  "dependencies": {
    "@poker-god/contracts": "1.0.0",
    "@poker-god/domain-poker": "1.0.0",
    "@poker-god/solver-core": "1.0.0",
    "expo": "~54.0.33",
    "expo-av": "~16.0.8",
    "expo-file-system": "~19.0.21",
    "expo-linear-gradient": "~15.0.8",
    "expo-speech": "~14.0.8",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2",
    "eas-cli": "^16.0.0"
  },
  "private": true
}
