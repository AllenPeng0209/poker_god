# AI coach voice provider
# auto: prefer OpenAI omni audio when key exists, else Qwen + Qwen TTS, else local TTS
# openai_omni: force OpenAI omni audio
# qwen: force Qwen text + Qwen TTS
EXPO_PUBLIC_COACH_VOICE_PROVIDER=auto

# Qwen / DashScope chat
EXPO_PUBLIC_QWEN_API_KEY=
EXPO_PUBLIC_QWEN_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
EXPO_PUBLIC_QWEN_MODEL=qwen3-max

# Qwen / DashScope TTS (optional overrides)
EXPO_PUBLIC_QWEN_TTS_ENDPOINT=
EXPO_PUBLIC_QWEN_TTS_BASE_URL=https://dashscope.aliyuncs.com/api/v1
EXPO_PUBLIC_QWEN_TTS_MODEL=qwen3-tts-flash
EXPO_PUBLIC_QWEN_TTS_VOICE=
EXPO_PUBLIC_QWEN_TTS_FORMAT=mp3
EXPO_PUBLIC_QWEN_TTS_SAMPLE_RATE=24000

# Bailian aliases (compatible with Wondera env)
EXPO_PUBLIC_BAILIAN_API_KEY=
EXPO_PUBLIC_BAILIAN_ENDPOINT=https://dashscope.aliyuncs.com
EXPO_PUBLIC_BAILIAN_WORKSPACE_ID=
EXPO_PUBLIC_BAILIAN_USE_WORKSPACE=

# OpenAI omni audio
EXPO_PUBLIC_OPENAI_API_KEY=
EXPO_PUBLIC_OPENAI_ENDPOINT=https://api.openai.com/v1/chat/completions
EXPO_PUBLIC_OPENAI_OMNI_MODEL=gpt-4o-audio-preview
EXPO_PUBLIC_OPENAI_VOICE=alloy
EXPO_PUBLIC_OPENAI_AUDIO_FORMAT=mp3
